# If [ $1 -ne 0 ], print $2 and exit.
Exit_if_failed() {
    if [ $1 -ne 0 ]; then[20;9Hecho -e "$2"[21;9Hexit 1
    fi
}

# eval "$1". If it failed, print $2 and exit.
Eval_and_Exit_if_failed() {
    eval "$1"
    Exit_if_failed $? "$2"
}
